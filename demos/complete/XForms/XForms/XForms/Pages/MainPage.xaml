<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="XForms.Pages.MainPage"
             xmlns:pages="clr-namespace:XForms.Pages"
             Title="XForms">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Color creator" Clicked="MenuItemClick"
                    StyleId="{x:Static pages:MainPage.ColorPage}" />
            <Button Text="Dialer" Clicked="MenuItemClick"
                    StyleId="{x:Static pages:MainPage.DialerPage}" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>